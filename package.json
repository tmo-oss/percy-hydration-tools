{
  "name": "percy-cake-hydration-tools",
  "version": "1.0.11",
  "description": "Command line hydration utility scripts for files dehydrated for `Percy - Configuration As Kode Editor`",
  "repository": "https://gitlab.com/tmo-poc/hydration-tool",
  "main": "dist/index.js",
  "types": "dist/types/index.d.js",
  "bin": {
    "hydrate": "dist/hydrate.js",
    "compare-json": "dist/compare-json.js"
  },
  "scripts": {
    "docs": "typedoc --out docs/ --excludePrivate --exclude '**/index.ts' src/lib",
    "tsc": "tsc",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "jest --coverage",
    "compare-json": "ts-node src/compare-json.ts",
    "hydrate": "ts-node src/hydrate.ts",
    "compare-json.js": "node dist/compare-json.js",
    "hydrate.js": "node dist/hydrate.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "config": "^3.3.6",
    "dotenv": "^10.0.0",
    "fs-extra": "^9.1.0",
    "git-remote-origin-url": "^3.1.0",
    "js-yaml": "^4.1.0",
    "jsondiffpatch": "^0.4.1",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "node-plantuml": "^0.9.0",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/chai-as-promised": "^7.1.3",
    "@types/config": "0.0.38",
    "@types/fs-extra": "^8.1.1",
    "@types/jest": "^26.0.23",
    "@types/js-yaml": "^4.0.1",
    "@types/lodash": "^4.14.168",
    "@types/minimist": "^1.2.1",
    "@types/node": "^10.17.59",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.1",
    "eslint": "^7.25.0",
    "jest": "^26.6.3",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ]
}
